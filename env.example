# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Backend Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Server Configuration
PORT=3001
WS_PORT=3002
NODE_ENV=production

# LinkedIn Auto-Apply Configuration
# Note: LinkedIn credentials are stored securely in Supabase linkedin_credentials table
# with encryption and Row Level Security (RLS)
# These environment variables are only used for testing/development
LINKEDIN_EMAIL=your-linkedin-email
LINKEDIN_PASSWORD=your-linkedin-password
BROWSER_HEADLESS=true
BROWSER_SLOW_MO=1000
PAGE_LOAD_TIMEOUT=30000
ELEMENT_WAIT_TIMEOUT=10000

# Resume and Screenshot Configuration
RESUME_PATH=./resume.pdf
SCREENSHOT_DIR=./screenshots

# CORS Configuration
CORS_ORIGIN=https://your-frontend-domain.com

# Encryption Key (for LinkedIn credentials)
# Note: The application currently uses 'your-secret-key-here' as the default encryption key
# You can override this by setting ENCRYPTION_KEY environment variable
ENCRYPTION_KEY=your-secret-key-here
